Function DrawBGMenu()
    Select currbg
    Case -1
    Case 0
        DrawText "BG9:"+mapPropData[currlv, 0], 0, 0
    Case 1
        DrawText "BG8:"+mapPropData[currlv, 1], 0, 0
    Case 2
        DrawText "BG7:"+mapPropData[currlv, 2], 0, 0
    Case 3
        DrawText "BG6:"+mapPropData[currlv, 3], 0, 0
    Case 4
        DrawText "BG5:"+mapPropData[currlv, 4], 0, 0
    Case 5
        DrawText "BG4:"+mapPropData[currlv, 5], 0, 0
    Case 6
        DrawText "BG3:"+mapPropData[currlv, 6], 0, 0
    Case 7
        DrawText "BG2:"+mapPropData[currlv, 7], 0, 0
    Case 8
        DrawText "BG1:"+mapPropData[currlv, 8], 0, 0
    Case 9
        DrawText "BG0:"+mapPropData[currlv, 9], 0, 0
    Case 102
    Default
        DrawText "BG"+currbg+":"+mapPropData[currlv, currbg], 0, 0
    End Select
End Function

Function EndBGMenu()
End Function
